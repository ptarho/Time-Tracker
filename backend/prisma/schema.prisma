generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model TimeEntry {
  id          Int      @id @default(autoincrement())
  date        DateTime @db.Date
  project     String
  hours       Float
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@index([date])
}
